  "private": true,
  "dependencies": {
    "@coremedia/studio-client.core-icons": "2110.1.0",
    "@coremedia/studio-client.ext.ui-components": "2110.1.0",
    "@coremedia/studio-client.main.editor-components": "2110.1.0",
    "@coremedia/studio-client.main.feedback-hub-editor-components": "2110.1.0",
    "@jangaroo/ext-ts": "^1.0.3",
    "@jangaroo/runtime": "^1.0.0"
  },
  "devDependencies": {
    "@jangaroo/build": "^1.5.0",
    "@jangaroo/core": "^1.5.0",
    "@jangaroo/publish": "^1.5.0",
    "@jangaroo/run": "^1.5.0",
    "eslint": "^8.56.0",
    "rimraf": "^5.0.5"
  },
  "scripts": {
    "clean": "rimraf ./dist && rimraf ./build",
    "build": "jangaroo build",
    "watch": "jangaroo watch",
    "start": "jangaroo run",
    "package": "jangaroo package",
    "publish": "jangaroo publish",
    "lint": "eslint --fix \"src/**/*.ts\""
